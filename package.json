{
  "name": "@azirgopro/azirdesign",
  "version": "1.0.1",
  "description": "A React component library built with TypeScript, Tailwind CSS, and Radix UI",
  "keywords": [
    "react",
    "components",
    "ui",
    "typescript",
    "tailwind",
    "radix"
  ],
  "author": "Azirgo Company",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:azirgo-company/azirdesign.git"
  },
  "homepage": "https://github.com/azirgo-company/azirdesign#readme",
  "bugs": {
    "url": "https://github.com/azirgo-company/azirdesign/issues"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "import": "./dist/client.mjs",
      "require": "./dist/client.js"
    },
    "./leaflet.css": "./dist/leaflet.css",
    "./styles.css": "./dist/styles.css"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup && npm run build:css && npm run copy:leaflet",
    "build:css": "postcss ./src/styles.css -o ./dist/styles.css",
    "copy:leaflet": "cpy node_modules/leaflet/dist/leaflet.css dist/ --flat",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.13",
    "autoprefixer": "^10.4.21",
    "cpy-cli": "^5.0.0",
    "postcss": "^8.4.0",
    "postcss-cli": "^11.0.1",
    "rimraf": "^5.0.0",
    "tailwindcss": "^4.1.13",
    "tsup": "^8.5.0",
    "typescript": "^5.9.0"
  },
  "sideEffects": [
    "dist/styles.css",
    "dist/leaflet.css"
  ],
  "peerDependencies": {
    "@clerk/nextjs": ">=6.31.0",
    "@radix-ui/react-accordion": ">=1.1.0",
    "@radix-ui/react-alert-dialog": ">=1.1.0",
    "@radix-ui/react-avatar": ">=1.1.0",
    "@radix-ui/react-dialog": ">=1.1.0",
    "@radix-ui/react-dropdown-menu": ">=2.0.0",
    "@radix-ui/react-label": ">=2.1.0",
    "@radix-ui/react-select": ">=2.2.0",
    "@radix-ui/react-separator": ">=1.1.0",
    "@radix-ui/react-slot": ">=1.2.0",
    "@radix-ui/react-tooltip": ">=1.2.0",
    "@refinedev/core": "^4.57.11",
    "@refinedev/nestjs-query": ">=1.3.0",
    "@refinedev/react-table": "^5.6.17",
    "@refinedev/ui-types": ">=1.24.0",
    "@tanstack/react-table": ">=8.21.0",
    "camelcase-keys": ">=9.1.0",
    "class-variance-authority": ">=0.7.0",
    "@refinedev/react-hook-form": "^4.10.2",
    "leaflet": ">=1.9.0",
    "lucide-react": ">=0.452.0",
    "next": ">=15.0.0",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0",
    "react-hook-form": ">=7.62.0",
    "tailwind-merge": ">=2.2.0"
  },
  "dependencies": {
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-radio-group": "^1.3.8",
    "react-day-picker": "^9.9.0"
  }
}
